@#!/bin/sh
# $Id: morecat,v 1.1 2013-09-25 13:51:21-07 - - $
#
# Run
# headers like more but use cat -nv for line numbers.
#

for file in $*
do
   echo ::::::::::::::::
   echo $file
   echo ::::::::::::::::
   cat -nv $file
done
@
